{
  "expo": {
    "name": "Waqt",
    "slug": "Waqt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icons/adaptive-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icons/adaptive-icon.png",
        "monochromeImage": "./assets/images/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sarraf.Waqt"
    },
    "ios": {
      "icon": {
        "dark": "./assets/images/icons/ios-dark.png",
        "light": "./assets/images/icons/ios-light.png",
        "tinted": "./assets/images/icons/ios-tinted.png"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.sarraf.Waqt",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "favicon": "./assets/images/icons/ios-light.png",
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-navigation-bar",
      "expo-system-ui",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icons/splash-icon-dark.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/icons/splash-icon-light.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Waqt to use your location."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icons/notif.png",
          "color": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e43302db-1f3c-49ef-acc0-66af572e65c1"
      }
    }
  }
}
